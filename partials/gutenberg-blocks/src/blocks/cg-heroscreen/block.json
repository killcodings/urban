{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cg/heroscreen",
	"version": "0.1.0",
	"title": "Heroscreen",
	"category": "cg-blocks",
	"description": "Example block scaffolded with Create Block tool.",
	"keywords": ["text", "box"],
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "heroscreen",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": ["file:./view.js", "file:./view_2.js"],
	"attributes": {
		"text" : {
			"type" : "string",
			"source" : "html",
			"selector": "p"
		},
		"isBage" : {
			"type" : "boolean",
			"default": true
		},
		"urlMedia": {
			"type": "string"
		}
	}
}
