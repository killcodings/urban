{
	"name": "block-starter-kit",
	"version": "0.1.0",
	"description": "Example block scaffolded with Create Block tool.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"build": "wp-scripts build --webpack-src-dir=src/blocks --output-path=dist",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start",
		"add": "npx @wordpress/create-block --namespace cg --category cg-blocks --variant dynamic --no-plugin",
		"add-static": "npx @wordpress/create-block --namespace cg --category cg-blocks --no-plugin"
	},
	"devDependencies": {
		"@wordpress/scripts": "^26.9.0"
	}
}
